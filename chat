    @ManyToMany(fetch = FetchType.LAZY)
    @JoinTable(name = "\"9EQJ\".DELEGADO_ESCOPO_DELEGACAO", joinColumns = @JoinColumn(name = "DLED_SQ_DELEGADO"),
               inverseJoinColumns = @JoinColumn(name = "DLED_SQ_ESCOPO"))
    private List<EscopoDelegacao> escoposDelegacao = new ArrayList<>();
	
